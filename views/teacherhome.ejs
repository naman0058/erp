                    <th data-field="id">Name</th>
                      <th data-field="name" data-sortable="true">Roll Number</th>
                      <th>Mobile Number</th>
                      <th>Email</th>
                      <th>Image</th>
                    </thead>
                    <tbody>
                        <% for(i=0; i<result[4].length; i++) { %>

                      <tr>
                        <td><%= result[4][i].name.toUpperCase() %></td>
                        <td><%= result[4][i].rollnumber %></td>
                        <td><%= result[4][i].number %></td>
                        <td><%= result[4][i].email %></td>
                        <td><img src='/images/<%= result[4][i].logo %>'style="width: 50px; height: 50px"></td>
                      </tr>
                      <% } %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
         <div class="col-lg-12 col-md-12">
